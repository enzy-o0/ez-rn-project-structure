{
  "name": "react-native-project-structure",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "storybookE": "start-storybook -p 7007 -h 192.168.35.19",
    "storybook": "start-storybook -p 7007 -h 192.168.35.22",
    "build-storybook": "build-storybook",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:apk": "cd android && ENVFILE=.env.dev ./gradlew app:assembleRelease --stacktrace",
    "android:aab": "cd android && ENVFILE=.env.prod ./gradlew app:bundleRelease --stacktrace",
    "android:dev": "ENVFILE=.env.dev react-native run-android",
    "android:prod": "ENVFILE=.env.prod react-native run-android"
  },
